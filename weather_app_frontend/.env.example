# API configuration for Weather App Frontend
# Copy this file to .env and fill in values as needed.

# Base URL for the weather API, e.g., https://api.openweathermap.org/data/2.5
REACT_APP_WEATHER_API_BASE=

# API Key for the weather API
REACT_APP_WEATHER_API_KEY=

# Optional: Enable demo mode when no API key is configured (true/false)
REACT_APP_WEATHER_DEMO_MODE=true
